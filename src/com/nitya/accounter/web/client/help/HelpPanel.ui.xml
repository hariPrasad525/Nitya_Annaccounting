<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.helpPanel {
			
		}
		
		.helpHPanel {
			background: none repeat scroll 0 0 #1A5158;
		}
		
		.helpLabel {
			
		}
		
		.helpFrame {
			
		}
		
		.helpButton {
			
		}
	</ui:style>
	<g:HTMLPanel>
		<g:FlowPanel ui:field="verticalPannel" styleName="{style.helpPanel}">
				<g:FlowPanel styleName="{style.helpHPanel}"
					ui:field="horizontalhelpPanel" width="100%">
					<g:Label styleName="{style.helpLabel}" ui:field="helpLabel"></g:Label>
						<g:ToggleButton ui:field="button" styleName="{style.helpButton}">
							<g:upFace>
							</g:upFace>
							<g:downFace>
							</g:downFace>
						</g:ToggleButton>
				</g:FlowPanel>
			<g:Frame ui:field="frame" styleName="{style.helpFrame}"></g:Frame>
		</g:FlowPanel>
		<!-- <g:Button styleName="{style.important}" ui:field="button" /> -->
	</g:HTMLPanel>
</ui:UiBinder> 