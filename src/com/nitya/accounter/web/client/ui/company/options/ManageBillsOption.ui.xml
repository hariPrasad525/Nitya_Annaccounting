<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel styleName="companyInfoPanel">
		<g:Label ui:field="managingBillLabelItem" styleName="header" />
		<g:Label ui:field="managingBilldescritionLabel" />
		<g:FlowPanel styleName="manageBillsOweRadioButtonsPanel">
			<g:RadioButton name="managingBillRadioButton" ui:field="managingBillYesRadioButton" />
			<g:RadioButton name="managingBillRadioButton" ui:field="managingBillNoRadioButton"
				checked="true" />
		</g:FlowPanel>
		<g:FlowPanel>
			<g:CheckBox ui:field="isPriceLevelsEnabled" />
		</g:FlowPanel>
		<g:CheckBox ui:field="purchaseOrderCheckBox" />
	</g:FlowPanel>
</ui:UiBinder> 