<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:Label ui:field="customerCommentLabel" styleName="refer_customer" />
		<g:FlowPanel styleName="refer_panel">
			<g:Label styleName="setup_label" ui:field="customerLabel" />
			<g:ListBox ui:field="customerListBox" styleName="refer_listbox" />
		</g:FlowPanel>
		<g:Label ui:field="supplierCommentLabel" styleName="refer_supplier" />
		<g:FlowPanel styleName="refer_panel">
			<g:Label styleName="setup_label" ui:field="vendorsLabel" />
			<g:ListBox ui:field="vendorListBox" styleName="refer_listbox" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 