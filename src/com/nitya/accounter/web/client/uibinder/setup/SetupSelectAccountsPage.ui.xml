<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel" styleName="setup_common_head">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel">
			<g:Label styleName="setup_label" ui:field="expensesInfo" />
			<g:Label styleName="setup_label" wordWrap="true" ui:field="recommendedInfo" />
			<g:ScrollPanel height="300px" styleName="review_expense_account">
				<g:FlexTable ui:field="accountsTable" />
			</g:ScrollPanel>
			<g:FlowPanel>
				<g:Button enabled="true" ui:field="restoreButton" />
				<g:Button ui:field="selectAllButton" />
			</g:FlowPanel>
			<g:Anchor wordWrap="true" ui:field="expensesLink" styleName="expense_question_link" />
			<g:Label styleName="setup_label" wordWrap="true" ui:field="expensesNote" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 