<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel">
			<g:Label styleName="setup_label" ui:field="congratulationInfo" />
			<g:Label styleName="setup_label" ui:field="congratulationLabel1" />
			<g:Label styleName="setup_label" ui:field="congratulationLabel2" />
		</g:FlowPanel>
		<!-- Add GWT widgets here. Enter < and hit Ctrl-Space to auto-complete 
			widget names. The ui:field attribute binds an element to a field in the owner 
			class. See the GWT docs on UI Binder for more details. -->

	</g:FlowPanel>
</ui:UiBinder> 