<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel" width="" height="">
			<g:CheckBox styleName="setup_check_box" ui:field="enableBox" />
			<g:Label styleName="setup_label" wordWrap="true" ui:field="encryptionInfo" />
			<g:FlowPanel ui:field="companyInfoField" styleName="company_Info_table">
				<g:FlowPanel styleName="password-panel">
					<g:Label styleName="setup_label" ui:field="passwordLabel" />
					<g:PasswordTextBox ui:field="passwordField" />
				</g:FlowPanel>
				<g:FlowPanel styleName="conform-password-panel">
					<g:Label styleName="setup_label" ui:field="conformPasswordLabel" />
					<g:PasswordTextBox ui:field="conformPasswordField" />
				</g:FlowPanel>
				<g:FlowPanel styleName="hint-panel">
					<g:Label styleName="setup_label" ui:field="hintLabel" />
					<g:TextBox styleName="setup_textbox" ui:field="hintField" />
				</g:FlowPanel>
			</g:FlowPanel>
			<g:Label styleName="setup_label" wordWrap="true" ui:field="passwordInfo" />
			<g:Hyperlink ui:field="moreInfoLink" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 